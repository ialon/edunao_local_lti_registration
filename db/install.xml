<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/lti_registration/db" VERSION="20220211" COMMENT="XMLDB file for Moodle local/lti_registration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="enrol_lti_app_registration_pending" COMMENT="Details of application that has been registered but are pending authorization">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="registrationid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="openid" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="regtoken" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
